<script>
  // Función para mostrar el Sweet Alert cuando el usuario llega al footer
  function mostrarSweetAlert() {
    Swal.fire({
      title: "¡Excelente! ahora sabes mucho más acerca de nosotros.",
      showCloseButton: true, 
      text: "Te invitamos a seguir recorriendo nuestra web",   
      icon:"success",
      confirmButtonText: "¡Continuar!" // Cambiado el texto del botón de confirmación a "OK"
    }).then((result) => {
      // Verificar si el usuario hizo clic en el botón "OK"
      if (result.isConfirmed) {
        // Realizar la redirección a la ruta deseada
        window.location.href = "https://www.encope.gob.ar/noticias";
      }
    });
  }

  // Función que verifica la posición del scroll y muestra la Sweet Alert si el usuario llega al footer
  function verificarScroll() {
      var footer = document.getElementById("footer");
      var distanciaHastaElFooter = footer.offsetTop - window.innerHeight;

      if (window.scrollY >= distanciaHastaElFooter) {
          mostrarSweetAlert();
          // Remover el evento de scroll después de mostrar la Sweet Alert
          window.removeEventListener("scroll", verificarScroll);
      }
  }

  // Agregar el evento de scroll para llamar a la función verificarScroll
  window.addEventListener("scroll", verificarScroll);

  // Incluir la librería de Sweet Alert (asegúrate de incluirla antes de este script)
  // Puedes obtenerla desde el sitio oficial: https://sweetalert2.github.io/
  // Ejemplo: <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
</script>

