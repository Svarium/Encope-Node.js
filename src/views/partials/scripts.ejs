<!-- Alert cierre sesion -->
<script>
    function showAlertAndLogout(event) {
     event.preventDefault();
 
     Swal.fire({
       title: '¿Estas seguro de  salir?',
       icon: 'warning',
       showCancelButton: true,
       confirmButtonColor: '#3085d6',
       cancelButtonColor: '#d33',
       confirmButtonText: 'Si, cerrar sesión'
     }).then((result) => {
       if (result.isConfirmed) {
         // Si el usuario confirmó el alert, envía el formulario manualmente
         const logout = document.getElementById("logoutUser");
         let timerInterval
 Swal.fire({
   title: '¡Esperamos que vuelvas pronto!',
   icon:'success',
   html: 'cerrando sesion en <b></b>...',
   timer: 5000,
   timerProgressBar: true,
   didOpen: () => {
     Swal.showLoading()
     const b = Swal.getHtmlContainer().querySelector('b')
     timerInterval = setInterval(() => {
       b.textContent = Swal.getTimerLeft()
     }, 100)
   },
   willClose: () => {
     clearInterval(timerInterval)
   }
 }).then((result) => {
   /* Read more about handling dismissals below */
   if (result.dismiss === Swal.DismissReason.timer) {
     console.log('I was closed by the timer')
   }
 })
 
         // Retraso de 5 segundos antes de enviar el formulario y cambiar la página
         setTimeout(function() {
           logout.submit();
         }, 5200); // 2000 milisegundos = 2 segundos
       }
     });
   }
   
   
   
     
   
 </script>



